<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PagO_Joined_Succes_Activity"
    android:background="@drawable/pag15_ampliar_con_icono">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guia_izquierda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.01" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guia_derecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.99" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelin_top_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.4534231" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_inferior_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.49943882" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guia_superior_btn_join"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.68" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guia_inferior_btnJoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelin_vertical_btn_join"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.21" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_vertical_drc_join"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.81" />



    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline_inferior_txt"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical_drc_join"
        app:layout_constraintStart_toStartOf="@+id/guidelin_vertical_btn_join"
        app:layout_constraintTop_toTopOf="@+id/guidelin_top_txt">


        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Joined Succesfully"
            tools:layout_editor_absoluteX="77dp"
            tools:layout_editor_absoluteY="9dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/guia_superior_btn_join"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical_drc_join"
        app:layout_constraintStart_toStartOf="@+id/guidelin_vertical_btn_join"
        app:layout_constraintTop_toTopOf="@+id/guideline_inferior_txt">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/pag15_marco_join_ampliado_photoroom"
        app:layout_constraintBottom_toTopOf="@+id/guia_inferior_btnJoin"
        app:layout_constraintEnd_toStartOf="@+id/guideline_vertical_drc_join"
        app:layout_constraintStart_toStartOf="@+id/guidelin_vertical_btn_join"
        app:layout_constraintTop_toTopOf="@+id/guia_superior_btn_join">

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>